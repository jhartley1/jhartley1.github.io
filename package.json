{
  "name": "jhartley1.github.io",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run build:eleventy && npm run build:tailwind",
    "watch": "npm-run-all --parallel watch:eleventy watch:tailwind",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "watch:tailwind": "npx tailwindcss -i ./src/input.css -o ./_site/output.css --watch",
    "build:tailwind": "npx tailwindcss -i ./src/input.css -o ./_site/output.css",
    "build:eleventy": "npx @11ty/eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
"npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.1"
  }
}
